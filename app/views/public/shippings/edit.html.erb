<div class="row　center">
  <div class="col-md-2"></div>


<div class="col-md-8">
    <h1 class="text-center">配送先編集</h1>

<%= form_for @shipping,:url => {:action => :update} do |f| %>

  <table class="table">
      <tr>
        <th>郵便番号(ハイフンなし)</th>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      <tr>
        <th>住所</th>
        <td><%= f.text_area :address %></td>
      </tr>
      <tr>
        <th>宛名</th>
        <td><%= f.text_field :name %></td>
      </tr>
  </table>

  <div class="">
    <%= f.submit '登録する', class: "btn btn-success" %>
  </div>
<% end %>
</div>

<div class="col-md-2"></div>
</div>